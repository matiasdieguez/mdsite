@page
<link rel="stylesheet" href="~/css/music.css" />
<br />
<div class="row" id="Bytecoder">
    <div class="col-md-4 center-block text-center">
        <img src="~/images/live.jpg" class="img-album" alt="Live Coding Sessions">
    </div>
    <div class="col-md-4">
        <h2>Live Coding Music Sessions</h2>
        <p>Source code <a href="https://github.com/matiasdieguez/live-coding">https://github.com/matiasdieguez/live-coding</a></p>
        <br/>
    </div>
    <div class="col-md-4">
        <h4>0001 - Sundown</h4>
        <p>
            <audio controls>
                <source src="https://matiasdieguez.azurewebsites.net/data/live/matias-dieguez-live-coding-sundown.mp3" type="audio/mp3">
            </audio>
        </p>
        <h4>0010 - Liven up reggae</h4>
        <p>
            <audio controls>
                <source src="https://matiasdieguez.azurewebsites.net/data/live/matias-dieguez-live-coding-liven-up.mp3" type="audio/mp3">
            </audio>
        </p>
        <h4>0011 - The Audio Manifest</h4>
        <p>
            <audio controls>
                <source src="https://matiasdieguez.azurewebsites.net/data/live/matias-dieguez-live-coding-the-audio-manifest.mp3" type="audio/mp3">
            </audio>
        </p>
        <h4>0100 - Micro Sound Rising</h4>
        <p>
            <audio controls>
                <source src="https://matiasdieguez.azurewebsites.net/data/live/matias-dieguez-live-coding-micro-sound-rising.mp3" type="audio/mp3">
            </audio>
        </p>
        <h4>0101 - Aves en streaming</h4>
        <p>
            <audio controls>
                <source src="https://matiasdieguez.azurewebsites.net/data/live/matias-dieguez-live-coding-aves-en-streaming.mp3" type="audio/mp3">
            </audio>
        </p>
    </div>
</div>
<br />
@section Scripts
{
    <script src="~/js/music.js"></script>
}
